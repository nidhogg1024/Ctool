{
    // Tauri v2 配置文件模板
    // ##version## 和 ##description## 由构建脚本替换
    "productName": "ctool",
    "version": "##version##",
    "mainBinaryName": "ctool",
    "identifier": "org.baiy.ctool-tauri",
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "frontendDist": "../dist/web"
    },
    "app": {
        "withGlobalTauri": true,
        "security": {
            "csp": null
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 650,
                "resizable": true,
                "title": "Ctool 程序开发常用工具",
                "width": 900
            }
        ]
    },
    "bundle": {
        "active": true,
        "category": "DeveloperTool",
        "copyright": "",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "longDescription": "##description##",
        "targets": [
            "deb",
            "appimage",
            "msi",
            "app",
            "dmg"
        ],
        "macOS": {},
        "linux": {
            "deb": {
                "depends": []
            }
        },
        "windows": {
            "digestAlgorithm": "sha256"
        }
    }
}
